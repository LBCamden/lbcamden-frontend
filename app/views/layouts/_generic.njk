{% block bodyEnd %}
  {% if legacy %}
    {% include "partials/legacyJavaScript.njk" %}
  {% endif %}
  <script src="/public/govuk-frontend.js"></script>
  <script src="/public/all.js"></script>
  <script>window.GOVUKFrontend.initAll()</script>
  <script>window.LBCamdenFrontend.initAll()</script>
