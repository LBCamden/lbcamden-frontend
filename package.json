    "sass": "^1.80.6",
    "sass-color-helpers": "^2.1.1",
    "sassdoc": "^2.7.3",
    "shuffle-seed": "^1.1.6",
    "standard": "^14.3.4",
    "storybook": "^8.3.6",
    "stylelint": "^14.6.1",
    "stylelint-config-gds": "^0.2.0",
    "stylelint-order": "^5.0.0",
    "vinyl-paths": "^3.0.1",
    "yaml": "^2.6.0",
    "yaml-loader": "^0.8.0",
    "yargs": "^15.4.1"
  },
  "optionalDependencies": {
    "chromatic": "^11.3.0",
    "fsevents": "*",
    "jest": "^29.7.0",
    "jest-axe": "^5.0.1",
    "jest-environment-jsdom": "^29.7.0",
    "jest-serializer-html": "^7.0.0",
    "puppeteer": "^21.6.1"
  },
  "browserslist": [
    ">0.1%",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Samsung versions",
    "Safari >= 9",
    "ie 9-11",
    "iOS >= 9"
  ],
  "standard": {
    "globals": [
      "page",
      "browser",
      "context",
      "jestPuppeteer"
    ],
    "ignore": [
      "/dist/**/*.js",
      "/package/**/*.js",
      "/package/**/*.mjs",
      "/src/lbcamden/vendor/polyfills/**/*.js",
      "cypress.config.js",
      "/cypress/**/*.js"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./config/jest-setup.js"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  },
  "readme": "ERROR: No README data found!",
  "_id": "lbcamden-frontend@0.2.6",
